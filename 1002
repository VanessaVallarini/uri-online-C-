using System;

namespace URI
{
    class Program
    {
        static void Main(string[] args)
        {
            double raio = Double.Parse(Console.ReadLine());
            double n = 3.14159;
            double area = n * Math.Pow(raio, 2);
            Console.WriteLine("A={0}", area.ToString("0.0000"));
        }
    }
}
